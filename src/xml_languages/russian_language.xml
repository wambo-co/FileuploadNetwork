<?xml version="1.0"?>
<main>
    <header name="Header">
        <name>FileUploadNetwork</name>
        <css>node_modules/bulma/css/bulma.css</css>
        <css>node_modules/bootstrap-icons-font/dist/bootstrap-icons-font.css</css>
        <css>node_modules/flag-icon-css/css/flag-icons.css</css>
        <css>assets/css/main.css</css>
        <js>src/js/test.js</js>
        <js>src/js/test.js</js>
    </header>
    <loginpage name="LoginPage">
        <username>Имя пользователя: </username>
        <password>Пароль: </password>
        <loginBtn>Войти</loginBtn>
        <qText>Еще не зарегистрированы? Зарегистрируйтесь сейчас!</qText>
    </loginpage>
    <regpage name="RegisterPage">
        <username>Имя пользователя: </username>
        <password>Пароль: </password>
        <email>Email: </email>
        <register>Регистр</register>
        <alreadyRegister>Вы уже зарегистрированы? Войдите сейчас!</alreadyRegister>
        <yourUsername>Ваше имя пользователя: </yourUsername>
        <yourPassword>Ваш пароль:</yourPassword>
        <yourEmail>Ваш электронный адрес:</yourEmail>
    </regpage>
    <fileServiceSettings name="FileServiceSettings">
        <allow>jpg</allow>
        <allow>png</allow>
        <allow>java</allow>
        <allowSizePerFile></allowSizePerFile>
        <uploadLocation></uploadLocation>
        <fileToBig></fileToBig>
        <wrongFileTyp></wrongFileTyp>
        <successfullyUploaded></successfullyUploaded>
    </fileServiceSettings>
</main>

